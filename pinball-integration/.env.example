# PostgreSQL Database Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to git!

# Database Connection
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pinball_analytics
DB_USER=your_database_user
